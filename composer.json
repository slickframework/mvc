{
  "name": "slick/mvc",
  "description": "MVC implementation for Slick Framework.",
  "minimum-stability": "dev",
  "license": "MIT",
  "version": "v1.2.0",
  "keywords": [
    "slick",
    "MVC",
    "MVC implementation"
  ],
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Slick\\Mvc\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slick\\Tests\\Mvc\\": "tests"
    }
  },
  "require": {
    "php" : ">=5.5",
    "slick/configuration": "^1.2",
    "slick/di": "^1.1|^2.0",
    "slick/form": "^1.2",
    "slick/orm": "^1.3@dev",
    "aura/router": "^3.0",
    "symfony/yaml": "^2.1",
    "filp/whoops": "^2.1",
    "slick/console": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4",
    "phpunit/php-code-coverage": "^2",
    "scrutinizer/ocular": "~1.1",
    "behat/behat": "^3.0.4",
    "behat/mink": "^1.7",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-extension": "^2.2"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev",
      "dev-develop": "2.0-dev"
    }
  }
}
